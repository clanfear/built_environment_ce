@article{gaysinaMaternalSmokingPregnancy2013,
  title = {Maternal {{Smoking During Pregnancy}} and {{Offspring Conduct Problems}}: Evidence {{From}} 3 {{Independent Genetically Sensitive Research Designs}}},
  shorttitle = {Maternal {{Smoking During Pregnancy}} and {{Offspring Conduct Problems}}},
  author = {Gaysina, Darya and Fergusson, David M. and Leve, Leslie D. and Horwood, John and Reiss, David and Shaw, Daniel S. and Elam, Kit K. and Natsuaki, Misaki N. and Neiderhiser, Jenae M. and Harold, Gordon T.},
