
@article{akmanMothersPostpartumPsychological2006,
  title = {Mothers' Postpartum Psychological Adjustment and Infantile Colic},
  author = {Akman, I. and Ku{\c s}{\c c}u, K. and {\"O}zdemir, N. and Yurdakul, Z. and Solakoglu, M. and Orhan, L. and Karabekiroglu, A. and {\"O}zek, E.},
  year = {2006},
  month = may,
  journal = {Archives of Disease in Childhood},
  volume = {91},
  number = {5},
  pages = {417--419},
  issn = {0003-9888, 1468-2044},
  doi = {10.1136/adc.2005.083790},
  abstract = {Background: Infantile colic is a common problem of early infancy. There is limited data on the relation between postpartum maternal psychological problems and colic. Aim:: To investigate whether infantile colic is associated with postpartum mood disorders or insecure adult attachment style of the mother. Methods: Seventy eight mothers and newborns were enrolled in this prospective, longitudinal study. Maternal depressive symptoms were screened with Edinburgh Postpartum Depression Score (EPDS) and maternal anxiety was assessed with State-Trait Anxiety Inventory (STAI). The Adult Attachment Scale was used to determine the attachment style of the mother. Infantile colic was defined according to Wessel criteria. Results: Infantile colic was present in 17 infants (21.7\%); 12.9\% of the mothers had an EPDS {$\geqslant$}13. The mean EPDS of the mothers whose infants had infantile colic (10.2{$\pm$}6.0) was significantly higher than that of the mothers of infants without colic (6.3{$\pm$}4.0). Among infants with infantile colic, 62.5\% had mothers who had insecure attachment style, whereas only 31.1\% of mothers had insecure attachment when the infant did not have infantile colic. Conclusion: Postpartum maternal depressive symptoms and insecure attachment style are associated with infantile colic. Screening and early intervention of postpartum depression might promote the health of both the mother and infant.},
  copyright = {Copyright 2006 Archives of Disease in Childhood},
  langid = {english},
  pmid = {16452109},
  keywords = {attachment style,infantile colic,postpartum depression},
  file = {C\:\\Users\\charles.lanfear\\Zotero\\storage\\9FU4FUBA\\Akman et al. - 2006 - Mothers’ postpartum psychological adjustment and i.pdf;C\:\\Users\\charles.lanfear\\Zotero\\storage\\66MRIATT\\417.html}
}

@article{allardReconsideringUrbanDisadvantaged2013,
  title = {Reconsidering the {{Urban Disadvantaged}}: The {{Role}} of {{Systems}}, {{Institutions}}, and {{Organizations}}},
  shorttitle = {Reconsidering the {{Urban Disadvantaged}}},
  author = {Allard, Scott W. and Small, Mario L.},
  year = {2013},
  month = may,
  journal = {The ANNALS of the American Academy of Political and Social Science},
  volume = {647},
  number = {1},
  pages = {6--20},
  issn = {0002-7162, 1552-3349},
  doi = {10.1177/0002716213479317},
  abstract = {The recent economic recession and a sluggish recovery have made conditions especially precarious for the most disadvantaged members of the urban poor population\textemdash those with criminal records, health conditions, undocumented status, or unstable housing. We argue that the fewer the resources to which people have access, the more their circumstances will depend on the organizations in which they participate, the systems in which these organizations operate, and the institutions governing the behavior of both. We call for a renewed focus on systems, institutions, and organizations among researchers who study urban disadvantage, and review a series of studies that show the promise of these perspectives.},
  langid = {english}
}

