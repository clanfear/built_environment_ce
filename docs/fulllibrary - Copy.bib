@article{obrienBrokenWindowsLow2013,
  title = {Broken {{Windows}} and {{Low Adolescent Prosociality}}: Not {{Cause}} and {{Consequence}}, but {{Co}}-Symptoms of {{Low Collective Efficacy}}},
  shorttitle = {Broken {{Windows}} and {{Low Adolescent Prosociality}}},
  author = {O'Brien, Daniel Tumminelli and Kauffman, Richard A.},
  year = {2013},
