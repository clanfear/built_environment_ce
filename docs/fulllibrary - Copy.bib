@article{browningEcologicalNetworksNeighborhood2017,
  title = {Ecological {{Networks}} and {{Neighborhood Social Organization}}},
  author = {Browning, Christopher R. and Calder, Catherine A. and Soller, Brian and Jackson, Aubrey L. and Dirlam, Jonathan},
  year = {2017},
